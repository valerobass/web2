; BIND reverse data file for local loopback interface
$TTL	604800
@	IN	SOA	dns.sistema.sol. root.sistema.sol. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.sistema.sol.
100	IN	PTR	dns.sistema.sol.
101     IN      PTR     tierra.sistema.sol.
